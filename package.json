{
  "name": "buer-web",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "bootstrap": "pnpm install",
    "//dev": "调试",
    "dev": "vite",
    "//build": "发布",
    "build": "vite build",
    "//update": "更新依赖包",
    "update": "pnpm upgrade --interactive --latest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "//lint": "代码检查",
    "lint": "eslint --ext .vue,.js,.jsx,.ts,.tsx ./ --max-warnings 0",
    "//lint-fix": "代码修复",
    "lint-fix": "eslint --ext .vue,.js,jsx,.ts,.tsx ./ --max-warnings 0 --fix",
    "//stylelint": "样式检查",
    "stylelint": "stylelint src/**/*.{html,vue,css,sass,less}",
    "//stylelint-fix": "样式修复",
    "stylelint:fix": "stylelint --fix src/**/*.{html,vue,css,sass,less}",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "14.1.0",
    "axios": "1.13.2",
    "bpmn-js": "18.10.1",
    "bpmn-js-bpmnlint": "0.23.0",
    "bpmn-js-connectors-extension": "0.4.6",
    "bpmn-js-properties-panel": "5.48.0",
    "bpmn-js-token-simulation": "0.38.2",
    "bpmnlint": "11.7.3",
    "crypto-js": "4.2.0",
    "echarts": "6.0.0",
    "echarts-gl": "2.0.9",
    "echarts-liquidfill": "3.1.0",
    "file-saver": "2.0.5",
    "highlight.js": "11.11.1",
    "lodash-es": "4.17.23",
    "moment": "2.30.1",
    "nprogress": "0.2.0",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "print-js": "1.6.0",
    "smooth-scrollbar": "8.8.4",
    "sortablejs": "1.15.6",
    "tdesign-vue-next": "1.18.0",
    "tinymce": "8.3.2",
    "tvision-color": "1.6.0",
    "uuid": "13.0.0",
    "vue": "3.5.27",
    "vue-draggable-plus": "0.6.1",
    "vue-router": "4.6.4",
    "vue3-colorpicker": "2.3.0",
    "vuex": "4.1.0",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.2.0",
    "@iconify/json": "2.2.431",
    "@ls-lint/ls-lint": "2.3.1",
    "@types/lodash-es": "4.17.12",
    "@types/mockjs": "1.0.10",
    "@types/node": "25.0.10",
    "@types/nprogress": "0.2.3",
    "@types/sortablejs": "1.15.9",
    "@unocss/eslint-plugin": "66.6.0",
    "@vitejs/plugin-legacy": "7.2.1",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitejs/plugin-vue-jsx": "5.1.3",
    "autoprefixer": "10.4.23",
    "consola": "3.4.2",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-format": "1.3.1",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-vue": "10.7.0",
    "eslint-plugin-vue-scoped-css": "2.12.0",
    "husky": "9.1.7",
    "increase-memory-limit": "1.0.7",
    "less": "4.5.1",
    "postcss": "8.5.6",
    "postcss-html": "1.8.1",
    "postcss-less": "6.0.0",
    "prettier": "3.8.1",
    "rimraf": "6.1.2",
    "rollup": "4.56.0",
    "rollup-plugin-visualizer": "6.0.5",
    "sass": "1.97.3",
    "scss": "0.2.4",
    "stylelint": "17.0.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "40.0.0",
    "stylelint-less": "4.0.0",
    "stylelint-order": "7.0.1",
    "terser": "5.46.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "unocss": "66.6.0",
    "unplugin-vue-components": "31.0.0",
    "vite": "7.3.1",
    "vite-plugin-compression2": "2.4.0",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-mock": "3.0.2",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vite-plugin-vue-inspector": "5.3.2",
    "vue-tsc": "3.2.3"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "4.50.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "npm run lint:fix"
    ],
    "*.{html,vue,css,sass,less}": [
      "npm run stylelint:fix"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
